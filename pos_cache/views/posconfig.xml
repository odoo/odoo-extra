<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="view_pos_config_form">
            <field name="name">pos.config.form.view</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_config_form" />
            <field name="arch" type="xml">
                <xpath expr="//button[@name='set_deprecate']" position="after">
                    <button name='recomputeCache' type="object" string="Recompute cache"/>
                </xpath>
                <xpath expr="//field[@name='sequence_id']" position="after">
                    <field name='cachecomputeuser_id' />
                    <field name='cachetime' />
                </xpath>
            </field>
        </record>

    </data>
</openerp>
